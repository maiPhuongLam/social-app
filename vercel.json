{
  "version": 2,
  "builds": [
    { "src": "./src/index.ts", "use": "@vercel/node" },
    { "src": "package.json", "use": "@vercel/node" },
    { "src": "prisma/schema.prisma", "use": "@prisma/cli" }
  ],
  "routes": [{ "src": "/(.*)", "dest": "/src/index.ts" }],
  "env": {
    "DATABASE_URL": "@prisma_url"
  }
}
